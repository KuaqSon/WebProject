<% include layouts/adminHeader %>

    <h2 class="page-title">Edit category</h2>
    <a href="/admin/categories" class="btn btn-primary">Back to Back to categories</a>

    <form method="POST" action="/admin/categories/edit-category/<%= id %>">

        <div class="form-group">
            <label for="">Category</label>
            <input type="text" class="form-control" name="title" value="<%= title %>" placeholder="Category">
        </div>
       

        <button class="btn btn-default">Edit</button>
    </form>

    <% include layouts/adminFooter %>